<h1>Listing participantes</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Ap paterno</th>
    <th>Ap manterno</th>
    <th>Sexo</th>
    <th>Tipo doc</th>
    <th>Num doc</th>
    <th>Direccion</th>
    <th>Distrito</th>
    <th>Correo</th>
    <th>Fec nac</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @participantes.each do |participante| %>
  <tr>
    <td><%= participante.nombre %></td>
    <td><%= participante.ap_paterno %></td>
    <td><%= participante.ap_manterno %></td>
    <td><%= participante.sexo %></td>
    <td><%= participante.tipo_doc %></td>
    <td><%= participante.num_doc %></td>
    <td><%= participante.direccion %></td>
    <td><%= participante.distrito %></td>
    <td><%= participante.correo %></td>
    <td><%= participante.fec_nac %></td>
    <td><%= link_to 'Show', participante %></td>
    <td><%= link_to 'Edit', edit_participante_path(participante) %></td>
    <td><%= link_to 'Destroy', participante, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Participante', new_participante_path %>
